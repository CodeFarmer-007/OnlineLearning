@model BuildOptionSample.ClassInfo
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h2>ClassName:@Model.className</h2>
    <h4>ClassNo:@Model.classNo</h4>

    <h1>StudentInfo</h1>

    @foreach (var item in Model.Info)
    {
        <h5>@item.name</h5>
        <h5>@item.age</h5>
    }
</body>
</html>
